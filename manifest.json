{
  "manifest_version": 2,
  "name": "Jira Ticket Devtools",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["*://*.atlassian.net/browse/*"],
    "js":      ["content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Jira Dev Tools",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs"
  ]
}
